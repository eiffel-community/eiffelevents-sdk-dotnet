{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/**/**.csproj"
          ],
          "src": "../../src",
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude":["articles/**.md"]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "articles/**.md"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Eiffel.NET",
      "_appFooter": "Eiffel.NET Docs 2021",
      "_disableContribution": true,
      "_appLogoPath":"images/dowhile-favicon.png",
      "_appFaviconPath": "images/dowhile-favicon.png",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc" : false
    }
  }
}