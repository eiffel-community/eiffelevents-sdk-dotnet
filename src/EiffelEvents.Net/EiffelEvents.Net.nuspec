<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>EiffelEvents.Net</id>
        <version>$version$</version>
        <title>EiffelEvents.Net</title>
        <authors>ICT Cube and doWhile team</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="expression">Apache-2.0</license>
        <projectUrl>https://github.com/eiffel-community/eiffelevents-sdk-dotnet/</projectUrl>
        <description>SDK for working with Eiffel events in .NET languages (C#, F#, VB.NET etc.)</description>
        <copyright>Copyright (c) 2021 ICT Cube, doWhile, and Eiffel Community collaborators</copyright>
        <contentFiles>
            <files include="**\*.json" buildAction="Content"  copyToOutput="true"/>
        </contentFiles>
        <dependencies>
            <dependency id="FluentResults" version="3.2.0" />
            <dependency id="Newtonsoft.Json" version="13.0.1" />
            <dependency id="Newtonsoft.Json.Schema" version="3.0.14"/>
        </dependencies>
        <releaseNotes>
            Upgrade dependency (FluentResults) to its latest versions
        </releaseNotes>
    </metadata>
    <files>
       <file src="Schemas\**" target="contentFiles\any\any\Schemas"/>
       <file src="bin\$configuration$\net6.0\EiffelEvents.Net.dll" target="lib\net6.0" />
    </files>
</package>